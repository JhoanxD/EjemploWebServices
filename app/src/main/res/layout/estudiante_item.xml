        android:layout_width="50dp"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/txtItemApellidoEstudiante"
        android:layout_width="50dp"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/txtItemCiudadEstudiante"
        android:layout_width="50dp"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/txtItemSemestreEstudiante"
